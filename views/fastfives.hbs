		<ul class="entry collapsible">
			<!-- <ul class="entry {d{splitDate weekUsed}sd} collapsible"> -->
		{{#each entries}}
		    <li>
		      <!-- <p class="blue-text accent-4-text"> -->
		      <div class="collapsible-header"><p><b>{{name}}</b></p></div>
		      <div class="collapsible-body">
		      	<!-- <span>Lorem ipsum dolor sit amet.</span> -->
				<p style="display: inline-block; margin: 0;"><b><u>URL:</u></b><a href="{{url}}">&nbsp;{{title}}</a></p>
				<p><b><u>Quote Used:</u></b>&nbsp;"{{actualQuote}}"</p>
				<p><b><u>Timestamp:</u></b>&nbsp;{{timestamp}}</p>
